{
    "name": "@impact-market/api",
    "version": "0.0.8",
    "description": "Decentralized impact-driven 2-sided marketplace to provide financial services to charities and vulnerable beneficiaries.",
    "scripts": {
        "start": "nodemon --config nodemon.json src/api/app.ts",
        "worker-dev": "nodemon -w src/worker src/worker/index.ts",
        "serve": "node dist/api/app.js",
        "worker": "node dist/worker/index.js",
        "build": "tsc -p .",
        "build:watch": "tsc -p . --watch",
        "postinstall": "yarn build",
        "prettier": "prettier --write \"./**/*.ts\"",
        "eslint": "eslint --fix \"./**/*.ts\"",
        "test": "mocha",
        "precoverage": "yarn build",
        "coverage": "nyc --reporter=html mocha",
        "precoverage:ci": "yarn build",
        "coverage:ci": "nyc --reporter=text-lcov mocha | coveralls",
        "test:watch": "mocha --watch",
        "clean": "rm -rf dist coverage .nyc_output"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/impactMarket/api.git"
    },
    "author": "impact-market",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/impactMarket/api/issues"
    },
    "homepage": "https://github.com/impactMarket/api#readme",
    "engines": {
        "node": "14.15.1"
    },
    "_moduleAliases": {
        "@controllers": "dist/api/controllers",
        "@services": "dist/api/services",
        "@validators": "dist/api/validators",
        "@models": "dist/db/models",
        "@logger": "dist/logger"
    },
    "dependencies": {
        "@sentry/node": "5.27.6",
        "@sentry/tracing": "5.27.6",
        "aws-sdk": "2.808.0",
        "axios": "0.19.2",
        "bignumber.js": "9.0.0",
        "body-parser": "1.19.0",
        "celebrate": "13.0.4",
        "cors": "2.8.5",
        "cron": "1.8.2",
        "dotenv": "8.2.0",
        "ethers": "5.0.24",
        "expo-server-sdk": "3.6.0",
        "express": "4.17.1",
        "helmet": "4.2.0",
        "jsonwebtoken": "8.5.1",
        "lodash": "4.17.20",
        "mathjs": "7.3.0",
        "module-alias": "2.2.2",
        "moment": "2.29.0",
        "morgan": "1.10.0",
        "multer": "1.4.2",
        "multer-s3": "2.9.0",
        "node-schedule": "1.3.2",
        "pg": "8.5.1",
        "pg-hstore": "2.3.3",
        "pg-native": "3.0.0",
        "sequelize": "6.3.5",
        "sequelize-cli": "6.2.0",
        "sha3": "2.1.2",
        "winston": "3.2.1"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "1.0.1",
        "@types/bluebird": "3.5.33",
        "@types/chai": "4.2.14",
        "@types/cors": "2.8.6",
        "@types/cron": "1.7.2",
        "@types/express": "4.17.9",
        "@types/jsonwebtoken": "8.5.0",
        "@types/mocha": "8.2.0",
        "@types/multer": "1.4.5",
        "@types/multer-s3": "2.7.8",
        "@types/node": "14.14.12",
        "@types/node-schedule": "1.3.0",
        "@types/pg": "7.14.7",
        "@types/sinon": "9.0.9",
        "@types/validator": "13.0.0",
        "@typescript-eslint/eslint-plugin": "3.4.0",
        "@typescript-eslint/parser": "3.4.0",
        "chai": "4.2.0",
        "coveralls": "3.1.0",
        "eslint": "7.3.1",
        "eslint-config-prettier": "6.11.0",
        "eslint-config-universe": "4.0.0",
        "ganache-cli": "6.12.1",
        "mocha": "8.2.1",
        "nodemon": "2.0.6",
        "nyc": "15.1.0",
        "prettier": "2.2.0",
        "sinon": "9.2.1",
        "source-map-support": "0.5.19",
        "swagger-jsdoc": "6.0.0",
        "swagger-ui-express": "4.1.6",
        "ts-node": "9.0.0",
        "typescript": "3.8.3",
        "web3": "1.2.9",
        "web3-eth-contract": "1.2.9"
    }
}
